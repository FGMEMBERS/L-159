<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>ehsi4.ac</path>

  <animation>
    <object-name>EHSI.bezel</object-name>
    <object-name>EHSI.screen</object-name>

    <object-name>EHSI.crs</object-name>
    <object-name>EHSI.crs1</object-name>
    <object-name>EHSI.crs2</object-name>
    <object-name>EHSI.crs3</object-name>
    <object-name>EHSI.gspd</object-name>
    <object-name>EHSI.gsp1</object-name>
    <object-name>EHSI.gsp2</object-name>
    <object-name>EHSI.gsp3</object-name>
    <object-name>EHSI.ticks</object-name>
    <object-name>EHSI.compass</object-name>
    <object-name>EHSI.vsi</object-name>
    <object-name>EHSI.hdg</object-name>
    <object-name>EHSI.hdg1</object-name>
    <object-name>EHSI.hdg2</object-name>
    <object-name>EHSI.hdg3</object-name>
    <object-name>EHSI.hdg-bug</object-name>
    <object-name>EHSI.crsneedle</object-name>
    <object-name>EHSI.crsneedle.1</object-name>
    <object-name>EHSI.ias1</object-name>
    <object-name>EHSI.ias2</object-name>
    <object-name>EHSI.ias3</object-name>
    <object-name>EHSI.vspn</object-name>
    <object-name>EHSI.vs1</object-name>
    <object-name>EHSI.vs2</object-name>
    <object-name>EHSI.vs3</object-name>
    <object-name>EHSI.vs4</object-name>
    <object-name>EHSI.alt1</object-name>
    <object-name>EHSI.alt2</object-name>
    <object-name>EHSI.alt3</object-name>
    <object-name>EHSI.alt4</object-name>
    <object-name>EHSI.alt5</object-name>
    <object-name>EHSI.ias</object-name> 
    <object-name>EHSI.vs</object-name>
    <object-name>EHSI.alt</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>EHSI.crs</object-name>
    <object-name>EHSI.crs1</object-name>
    <object-name>EHSI.crs2</object-name>
    <object-name>EHSI.crs3</object-name>
    <object-name>EHSI.gspd</object-name>
    <object-name>EHSI.gsp1</object-name>
    <object-name>EHSI.gsp2</object-name>
    <object-name>EHSI.gsp3</object-name>
    <object-name>EHSI.ticks</object-name>
    <object-name>EHSI.compass</object-name>
    <object-name>EHSI.vsi</object-name>
    <object-name>EHSI.hdg</object-name>
    <object-name>EHSI.hdg1</object-name>
    <object-name>EHSI.hdg2</object-name>
    <object-name>EHSI.hdg3</object-name>
    <object-name>EHSI.hdg-bug</object-name>
    <object-name>EHSI.crsneedle</object-name>
    <object-name>EHSI.crsneedle.1</object-name>
    <object-name>EHSI.ias1</object-name>
    <object-name>EHSI.ias2</object-name>
    <object-name>EHSI.ias3</object-name>
    <object-name>EHSI.vspn</object-name>
    <object-name>EHSI.vs1</object-name>
    <object-name>EHSI.vs2</object-name>
    <object-name>EHSI.vs3</object-name>
    <object-name>EHSI.vs4</object-name>
    <object-name>EHSI.alt1</object-name>
    <object-name>EHSI.alt2</object-name>
    <object-name>EHSI.alt3</object-name>
    <object-name>EHSI.alt4</object-name>
    <object-name>EHSI.alt5</object-name>
    <object-name>EHSI.ias</object-name> 
    <object-name>EHSI.vs</object-name>
    <object-name>EHSI.alt</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   1 </red>
      <green> 1 </green>
      <blue>  1 </blue>
    </emission>
  </animation>

    <animation>
  <type>scale</type>
  <x-offset>0.55</x-offset>
  <y-offset>0.55</y-offset>
  <z-offset>0.55</z-offset>
 </animation>

  <animation>
    <type>rotate</type>
    <object-name>EHSI.hdg-bug</object-name>
    <property>/autopilot/internal/fdm-heading-bug-error-deg</property>
    <center>
      <x-m> 0.000 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.007 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>EHSI.compass</object-name>
    <property>orientation/heading-magnetic-deg</property>
    <center>
      <x-m> 0.000 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.007 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!--  COURSE -->
  <animation>
    <type>rotate</type>
    <object-name>EHSI.crsneedle</object-name>
    <object-name>EHSI.crsneedle.1</object-name>
    <property>orientation/heading-magnetic-deg</property>
    <center>
      <x-m> 0.000 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.007 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>EHSI.crsneedle</object-name>
    <object-name>EHSI.crsneedle.1</object-name>
    <property>/instrumentation/nav/radials/selected-deg</property>
    <center>
      <x-m> 0.000 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.007 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>EHSI.crsneedle.1</object-name>
    <property>/instrumentation/nav/heading-needle-deflection</property>
    <interpolation>
      <entry><ind> -999 </ind><dep> -0.02 </dep></entry>
      <entry><ind>  -10 </ind><dep> -0.02 </dep></entry>
      <entry><ind>    0 </ind><dep>  0.00 </dep></entry>
      <entry><ind>   10 </ind><dep>  0.02 </dep></entry>
      <entry><ind>  999 </ind><dep>  0.02 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0.000 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.007 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>EHSI.vsi</object-name>
    <property>autopilot/internal/vert-speed-fpm</property>
    <interpolation>
      <entry><ind> -3000 </ind><dep> -31.0 </dep></entry>
      <entry><ind> -2000 </ind><dep> -24.0 </dep></entry>
      <entry><ind> -1000 </ind><dep> -17.5 </dep></entry>
      <entry><ind>     0 </ind><dep>   0.0 </dep></entry>
      <entry><ind>  1000 </ind><dep>  17.5 </dep></entry>
      <entry><ind>  2000 </ind><dep>  24.0 </dep></entry>
      <entry><ind>  3000 </ind><dep>  31.0 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0.000 </x-m>
      <y-m> 0.104 </y-m>
      <z-m> 0.007 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>EHSI.crs1</object-name>
    <property>/instrumentation/nav/radials/selected-deg</property>
    <factor>0.001</factor>
    <step>100</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>EHSI.crs2</object-name>
    <property>/instrumentation/nav/radials/selected-deg</property>
    <factor>0.01</factor>
    <step>10</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>EHSI.crs3</object-name>
    <property>/instrumentation/nav/radials/selected-deg</property>
    <factor>0.1</factor>
    <step>1</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!--  hdg bug -->
  <animation>
    <type>textranslate</type>
    <object-name>EHSI.hdg1</object-name>
    <property>/autopilot/settings/heading-bug-deg</property>
    <factor>0.001</factor>
    <step>100</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>EHSI.hdg2</object-name>
    <property>/autopilot/settings/heading-bug-deg</property>
    <factor>0.01</factor>
    <step>10</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>EHSI.hdg3</object-name>
    <property>/autopilot/settings/heading-bug-deg</property>
    <factor>0.1</factor>
    <step>1</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!--  groundspeed -->
  <animation>
    <type>textranslate</type>
    <object-name>EHSI.gsp1</object-name>
    <property>velocities/groundspeed-kt</property>
    <factor>0.001</factor>
    <step>100</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>EHSI.gsp2</object-name>
    <property>velocities/groundspeed-kt</property>
    <factor>0.01</factor>
    <step>10</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>EHSI.gsp3</object-name>
    <property>velocities/groundspeed-kt</property>
    <factor>0.1</factor>
    <step>1</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
    <!--  AP IAS+ -->
  <animation>
    <type>textranslate</type>
    <object-name>EHSI.ias1</object-name>
    <property>/autopilot/settings/target-speed-kt</property>
    <factor>0.001</factor>
    <step>100</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>EHSI.ias2</object-name>
    <property>/autopilot/settings/target-speed-kt</property>
    <factor>0.01</factor>
    <step>10</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>EHSI.ias3</object-name>
    <property>/autopilot/settings/target-speed-kt</property>
    <factor>0.1</factor>
    <step>1</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  
    <!--  AP ALT -->
  <animation>
    <type>textranslate</type>
    <object-name>EHSI.alt1</object-name>
    <property>/autopilot/settings/target-altitude-ft</property>
    <factor>0.00001</factor>
    <step>10000</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>EHSI.alt2</object-name>
    <property>/autopilot/settings/target-altitude-ft</property>
    <factor>0.0001</factor>
    <step>1000</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>EHSI.alt3</object-name>
    <property>/autopilot/settings/target-altitude-ft</property>
    <factor>0.001</factor>
    <step>100</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>EHSI.alt4</object-name>
    <property>/autopilot/settings/target-altitude-ft</property>
    <factor>0.01</factor>
    <step>10</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>EHSI.alt5</object-name>
    <property>/autopilot/settings/target-altitude-ft</property>
    <factor>0.1</factor>
    <step>1</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

    <!--  AP VS -->
    <!--  The VS numeric display requires otto.nas in the nasal folder and a call to it in the set file, to establish the vertical-speed-abs property -->
     <!-- POS/NEG -->
  <animation>
    <type>select</type>
    <object-name>EHSI.vspn</object-name>
      <condition>
       <less-than>
       <property>/autopilot/settings/vertical-speed-fpm</property>
       <value>0</value>
       </less-than>
      </condition>
  </animation>
  
  <animation>
    <type>textranslate</type>
    <object-name>EHSI.vs1</object-name>
    <property>/autopilot/settings/vertical-speed-abs</property>
    <factor>0.0001</factor>
    <step>1000</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>EHSI.vs2</object-name>
    <property>/autopilot/settings/vertical-speed-abs</property>
    <factor>0.001</factor>
    <step>100</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>EHSI.vs3</object-name>
    <property>/autopilot/settings/vertical-speed-abs</property>
    <factor>0.01</factor>
    <step>10</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>EHSI.vs4</object-name>
    <property>/autopilot/settings/vertical-speed-abs</property>
    <factor>0.1</factor>
    <step>1</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

   <!-- *** LoD *** -->
 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>5</max-m>
 </animation>
</PropertyList>

