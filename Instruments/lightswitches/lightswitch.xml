  </animation>
  
  <!-- Switch.002, Landing Lighting -->
  <animation>
    <type>material</type>
    <object-name>Switch.002</object-name>
    <object-name>base</object-name>
    <object-name>info</object-name>
    <emission>
      <red>   0.50 </red>
