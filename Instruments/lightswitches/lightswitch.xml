    </axis>
  </animation>

  <!-- Knob.FORM, Formation Lights -->
    <animation>
        <type>pick</type>
        <object-name>Knob.FORM</object-name>
        <action>
            <button>0</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>/sim/multiplay/generic/float[5]</property>
                <step>0.05</step>
                <min>0</min>
                <max>1</max>
            </binding>
        </action>
    </animation>
    
    <animation>
            <type>pick</type>
        <object-name>Knob.FORM</object-name>
        <action>
            <button>2</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>/sim/multiplay/generic/float[5]</property>
                <step>-0.05</step>
                <min>0</min>
                <max>1</max>
            </binding>
        </action>
    </animation>    
    
     <animation>
            <type>pick</type>
        <object-name>Knob.FORM</object-name>
        <action>
            <button>3</button>
            <repeatable>true</repeatable>
            <interval-sec>0.05</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>/sim/multiplay/generic/float[5]</property>
                <step>0.05</step>
                <min>0</min>
                <max>1</max>
            </binding>
        </action>
    </animation>    
    
    <animation>
            <type>pick</type>
        <object-name>Knob.FORM</object-name>
        <action>
            <button>4</button>
            <repeatable>true</repeatable>
            <interval-sec>-0.05</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>/sim/multiplay/generic/float[5]</property>
                <step>-0.05</step>
                <min>0</min>
                <max>1</max>
            </binding>
        </action>
    </animation>    
    
